{
  "name": "node-backend",
  "version": "1.0.0",
  "description": "APIs in typescript",
  "main": "server.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development nodemon",
    "build": "tsc",
    "knex": "knex",
    "migration": "knex --knexfile=./src/db/knexfile.js migrate:make",
    "migration:rollback": "knex migrate:down --knexfile=./src/db/knexfile.js",
    "migration:migrate": "knex --knexfile=./src/db/knexfile.js migrate:migrate",
    "seed": "knex --knexfile=./src/db/knexfile.js seed:make",
    "test": "cross-env NODE_ENV=test jest --coverage"
  },
  "author": "Roland Song <rolandsong328@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "bole": "^4.0.0",
    "chalk": "^4.1.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "glob": "^7.1.6",
    "helmet": "^4.4.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.17",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "objection": "^2.2.14",
    "sqlite3": "^5.0.1",
    "stream-file-archive": "^1.1.4",
    "through2": "^4.0.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/faker": "^5.1.6",
    "@types/helmet": "0.0.48",
    "@types/jest": "^26.0.20",
    "@types/knex": "^0.16.1",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.28",
    "@types/through2": "^2.0.36",
    "@types/uuid": "^8.3.0",
    "faker": "^5.4.0",
    "jest": "^24.8.0",
    "kill-port": "^1.6.1",
    "nodemon": "^2.0.7",
    "ts-jest": "^26.5.1",
    "ts-lint": "^4.5.1",
    "ts-node": "^9.1.1",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "typescript": "^4.1.5"
  }
}
